  <select id="selectByMchntCd" parameterType="java.lang.String" resultMap="BaseResultMap">
	select tr.chnl_id, tr.chnl_mchnt_cd 
	from (select * from tbl_txn_routing where int_trans_cd like '52%' and status = '1' and mchnt_cd = #{mchnt_cd,jdbcType=VARCHAR}) as tr
	inner join tbl_chnl_mchnt_info cmi on (((tr.chnl_id = cmi.chnl_id) and (tr.chnl_mchnt_cd = cmi.chnl_mchnt_cd) and (cmi.state = '1')))
	inner join tbl_chnl_mchnt_account_info acc on(((tr.chnl_id = acc.chnl_id) and (tr.chnl_mchnt_cd = acc.mchnt_cd) and (acc.state = '1')))
  </select>
